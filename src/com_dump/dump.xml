<?xml version="1.0" encoding="utf-8"?>
<extension version="3.0" type="component" method="upgrade">
	<name>Dump</name>
	<author>Mathias Verraes</author>
	<creationDate>09/10/2020</creationDate>
	<copyright>(c) Mathias Verraes 2006 - 2020, fork by garstud</copyright>
	<license>GNU/GPL</license>
	<authorEmail></authorEmail>
	<authorUrl>https://github.com/garstud/jdump</authorUrl>
	<version>1.3.2</version>
	<description>J!Dump -- Advanced print_r and var_dump replacer with DHTML tree display.</description>

	<scriptfile>installScript.php</scriptfile>

	<files folder="front">
		<filename>dump.php</filename>
		<filename>index.html</filename>
		<folder>views</folder>
